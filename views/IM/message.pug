        section(id='main').w100
            article(id='demo')
            article(id='components')
    script(src=__JS_PATH__ + '/webim/browser-polyfill.min.js')
    script(src=__JS_PATH__ + '/webim/webim.config.js')
    script(src=__JS_PATH__ + '/webim/strophe-1.2.8.js')
